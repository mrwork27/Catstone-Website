@@ -0,0 +1,108 @@
@import url('https://fonts.googleapis.com/css2?family=Oswald&display=swap');

header {
    box-sizing: border-box;
    margin: 0;
    padding: 0;
}


li, a, button {
    font-family: "Oswald", sans-serif;
    font-weight: 400;
    font-size: 18px;
    text-decoration: none;
}

li, a {
    color: #7f7f7f;
}

button {
    color: #ffffff;
}

header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 10px;
}

.logo {
    cursor:pointer;
}

.nav_links {
    list-style: none;
}

.nav_links li {
    display: inline-block;
}

.nav_links li a {
    transition: all 0.3s ease 0s;
}

